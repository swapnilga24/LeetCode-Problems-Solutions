https://leetcode.com/problems/maximum-number-of-words-found-in-sentences/submissions/

class Solution {
    public int mostWordsFound(String[] sentences) {
        int max=0;
        for(int i=0;i<sentences.length;i++){
            int a=0;
            String[] temp = sentences[i].split(" ");
            a = temp.length;
            if(a>max){
                max=a;
            }
        }
        return max;
    }
}
